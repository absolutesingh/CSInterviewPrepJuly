package first;
class Student{
	String name;
	int marks;
	boolean checkPass(){
		if(marks>40) 
			{
				return true;
			}
		else {
			return false;
		}
	}
	
}
public class ControlFlow {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Student s1 = new Student();
		s1.name = "Ram";
		s1.marks = 80;
		Student s2 = new Student();
		s2.name = "Sham";
		s2.marks = 35;
		System.out.println(s1.checkPass());
		
	}

}
