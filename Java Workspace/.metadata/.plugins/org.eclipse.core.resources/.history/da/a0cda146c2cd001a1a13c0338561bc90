package queue;
class MyQueue{
	final int size = 10;
	int arr[] = new int[size];
	int front = -1;
	int rear = -1;
	void enqueue(int data)
	{
		if(this.isEmpty())
		{
			front++;
//			rear++;
//			arr[rear] = data;
			arr[++rear]=data;
		}
		else {
//			rear++;
//			arr[rear]=data;
			arr[++rear]=data;
		}
	}
	int dequeue() //Remove and return the removed element
	{
//		int x = arr[front];
//		front++;
//		return x;
		return arr[front++];
	}
	boolean isEmpty()
	{
//		if(front==-1 && rear==-1) return true;
//		return false;
		return (front==-1 && rear==-1);
	}
	int peek()
	{
		return arr[front];
	}
}
public class ArrayImp {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
