package arrays;

public class PassingArray {
	static void addOne(int arr[])//Increase all elements of array by 1
	{
		for(int i=0;i<arr.length;i++)
		{
			arr[i]=i+1;
		}
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int arr[] = new int[5];
		for(int i = 0;i<arr.length;i++)
		{
			System.out.print(arr[i]+" ");
		}
		System.out.println();
		addOne(arr);
		for(int i = 0;i<arr.length;i++)
		{
			System.out.print(arr[i]+" ");
		}
		System.out.println();
		
	}

}
