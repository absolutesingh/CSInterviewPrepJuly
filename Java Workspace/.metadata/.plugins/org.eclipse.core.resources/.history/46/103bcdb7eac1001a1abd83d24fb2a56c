package abstraction;
interface Shape{
	double pi = 3.14; //public, static, final
	int getSquare(int r); //public and abstract
}
class Circle implements Shape{
	//Overridding function must  havee higher or equal accessibility
	public int getSquare(int r)
	{
		return r*r;
	}
	
}
public class InterfaceStudy {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println(Shape.pi); //Proof of Static
		
	}

}
