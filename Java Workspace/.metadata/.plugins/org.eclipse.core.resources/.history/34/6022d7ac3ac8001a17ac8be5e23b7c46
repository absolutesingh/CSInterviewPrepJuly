class Node{
	int data;
	Node next;
	Node(int data)
	{
		this.data = data;
		this.next = null;
	}
}
public class MyLL {
	static void print(Node head)
	{
		while(head!=null)
		{
			System.out.print(head.data+" ");
			head = head.next;
		}
		System.out.println();
	}
	static void insertAtHead(Node head, int data)
	{
		Node temp = new Node(data); // Create new Node
		temp.next = head;
		head = temp;
		print(head);
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Node head = new Node(1);
		head.next = new Node(2);
		head.next.next = new Node(3);
		//Traversing the LL
		Node temp = head;
		while(temp!=null)
		{
			System.out.print(temp.data+" ");
			temp = temp.next;
		}
		System.out.println();
		print(head);
		insertAtHead(head,4);
		print(head);
	}

}
