package keywords;
class A
{
	final int x; //Value once assigned cannot be changed
	//Values can be assigned only at the time of declaration or inside constructors.
	A()
	{
		x = 5;
	}
	final void fun() //Final meethods cannot be overridden in subclasses
	{
		System.out.println("Fun of A");

	}
}
class B extends A
{
//	void fun()
//	{
//		System.out.println("Fun of B");
//	}
}
