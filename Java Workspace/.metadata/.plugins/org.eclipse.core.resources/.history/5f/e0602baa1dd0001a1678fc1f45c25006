package setsAndMaps;
import java.util.*;
public class SetImp {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		HashSet<Integer> s = new HashSet<>(); //HashSet is the class that implements the set interface
		System.out.println(s.size());
		s.add(1);
		s.add(2);
		s.add(3);
		System.out.println(s.size());
		System.out.println(s.contains(2));
		s.remove(2);
		System.out.println(s.size());
		System.out.println(s.contains(2));
		System.out.println(s.contains(3));
	}

}
